    "definitions": {
        "Error": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "Similar to the http status code",
                    "type": "integer",
                    "x-order": "1",
                    "example": 400
                },
                "type": {
                    "description": "The type of error",
                    "type": "string",
                    "enum": [
                        "validation",
                        "database",
                        "entity"
                    ],
                    "x-order": "2",
                    "example": "validation"
                },
                "message": {
                    "description": "A human-readable error message",
                    "type": "string",
                    "x-order": "3",
                    "example": "Invalid request Body"
                },
                "details": {
                    "description": "Additional details about the error, omitted if empty",
                    "type": "array",
                    "items": {
                        "type": "object"
                    },
                    "x-order": "4"
                },
                "correlation_id": {
